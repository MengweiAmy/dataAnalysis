function r = CalculateWPMForGroup(group,clickGroup)
    tGazStgrp1= group(0,10);
    tGazEndgrp1 = group(length(group),10);
    grp1Period = tGazStgrp1 - tGazEndgrp1;
    ids = []
    for c = 1: length(clickGroup)
        if strcmp(char(str(c)),'bksp') == 0
            ids = cat(1,ids,c);
         end
    end
    r = WPM(grp1Period)
end